2772 citations (Crossref/DOI) [2025-04-08]},
	pages = {352--373},
	file = {Tulving-1973-Encoding specificity and retrieva:/Users/Liam1/Zotero/storage/J7P5IV7H/Tulving-1973-Encoding specificity and retrieva.pdf:application/pdf},
}

@article{craikDepthProcessingRetention1975,
	title = {Depth of processing and the retention of words in episodic memory},
