	journal = {Neuropsychologia},
	author = {Renoult, Louis and Tanguay, Annick and Beaudry, Myriam and Tavakoli, Paniz and Rabipour, Sheida and Campbell, Kenneth and Moscovitch, Morris and Levine, Brian and Davidson, Patrick S.R.},
