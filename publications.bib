	pmcid = {PMC8609962},
	note = {18 citations (Crossref/DOI) [2025-05-21]
17 citations (Crossref/DOI) [2025-04-09]
17 citations (Crossref/DOI) [2025-04-08]},
	keywords = {Autobiographical memory, Aging, Episodic memory, Direct retrieval, Generative retrieval, /unread},
	pages = {422--437},
