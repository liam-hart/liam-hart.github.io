	journal = {Psychology and Aging},
	author = {Gerstorf, Denis and Drewelies, Johanna and Duezel, Sandra and Smith, Jacqui and Wahl, Hans-Werner and Schilling, Oliver K. and Kunzmann, Ute and Siebert, Jelena S. and Katzorreck, Martin and Eibich, Peter and Demuth, Ilja and Steinhagen-Thiessen, Elisabeth and Wagner, Gert G. and Lindenberger, Ulman and Heckhausen, Jutta and Ram, Nilam},
	month = dec,
