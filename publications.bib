	month = feb,
	year = {2020},
	pmid = {31928030},
	note = {66 citations (Crossref/DOI) [2025-05-23]},
	keywords = {Humans, Aged, Female, Male, Memory, Episodic, Middle Aged, /unread, Healthy Aging},
	pages = {124--142},
	file = {Submitted Version:/Users/Liam1/Zotero/storage/SH7D2CGP/Korkki et al. - 2020 - Healthy ageing reduces the precision of episodic memory retrieval.pdf:application/pdf},
